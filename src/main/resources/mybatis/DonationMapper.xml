<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.snsfood.admin.mapper.DonationMapper">

    <select id="selectListCount" resultType="long">
        select count(*)
        from donation
        where 1 =1;
    </select>

    <select id="selectList" resultType="com.example.snsfood.admin.dto.DonationDto">
        select *
        from donation
        where 1 = 1;

    </select>


</mapper>
